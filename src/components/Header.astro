<!-- Estilos CSS -->
<style>
  
header {
  background-color: #08070b;
  padding: 20px 0;
  width: 100%;
}

.container {
  display: flex;
  justify-content: space-between;
  align-items: center;
  max-width: 1200px;
  margin: 0 auto;
  padding: 0 20px;
}

.logo {
  font-size: 24px;
  font-weight: bold;
  color: #fff;
  text-decoration: none;
}
nav{
  margin:0 auto;
  font-family: Biotif, sans-serif;
}
.nav-list {
  display: flex;
  list-style: none;
  top:5px;
}

.nav-list a {
  color: #8f9ba8;
  text-decoration: none;
  font-size: 12px;
  padding: 20px;
  background-color: #08070b;
  border-radius:10px;
  transition: all 0.2s ease-out;
  
}

.nav-list a:hover {
  color:#f2f2f2;
  background-color: #212024;
  border-radius: 8px;
}

.button {
  background-color: #555;
  border-radius: 5px;
}

.button:hover {
  background-color: #777;
}

</style>

